<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<izpack:shortcuts version="5.0"
                  xmlns:izpack="http://izpack.org/schema/shortcuts"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">
    <!--skipIfNotSupported/-->
    <programGroup defaultName="Semantic Search"
                  location="startMenu"/>

    <shortcut name="Semantic Search" target="C:\Program Files\Semantic Search\Semantic Search.exe" description="Creating Gui Icon on the Desktop"
              workingDirectory="$INSTALL_PATH" iconFile="C:\Program Files\Semantic Search\Frontrail_Front.ico" initialState="normal" programGroup="yes" desktop="yes" applications="yes"
              startMenu="yes" startup="no">

        <createForPack name="Core"/>
    </shortcut>
    <shortcut name="Semantic Search - Uninstaller" target="C:\Program Files\Semantic Search\Uninstaller\uninstaller.jar" description="Creating Gui Icon on the Desktop"
              workingDirectory="$INSTALL_PATH" iconFile="C:\Program Files\Semantic Search\Frontrail_Front.ico" initialState="normal" programGroup="yes" desktop="yes" applications="yes"
              startMenu="yes" startup="no">
        <createForPack name="Core"/>
    </shortcut>
</izpack:shortcuts>